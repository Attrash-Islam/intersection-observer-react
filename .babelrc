{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    [
      "transform-react-remove-prop-types",
      {
        "mode": "wrap"
      }
    ]
  ],
  "env": {
    "dev": {
      "presets": [
        "es2015",
        "stage-0",
        "react-hmre"
      ]
    },
    "development": {
      "presets": [
        "es2015",
        "stage-0"
      ]
    },
    "modules": {
      "presets": [
        "es2015",
        "stage-0"
      ]
    },
    "es6": {
      "presets": [
        [
          "es2015",
          {
            "modules": false
          }
        ],
        "stage-0"
      ]
    },
    "test": {
      "presets": [
        "es2015",
        "stage-0"
      ]
    },
  }
}
